(:boundscheck
 (base <- eax)
 (offset <- edx)
 (length <- (mem eax 0))
 
 (eax <- offset)
 (call :decode)
 (decodedoffset <- eax)
 
 (cjump 0 <= decodedoffset :valid1 :invalid)
 
 :valid1
 
 (cjump decodedoffset < length :valid2 :invalid)
 
 :valid2
 (decodedoffset *= 4)
 (decodedoffset += 4)
 (base += decodedoffset)
 (eax <- base)
 (return)
 
 :invalid
 (eax <- (array-error base offset))
 (return))
