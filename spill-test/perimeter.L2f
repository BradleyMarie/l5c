(((edi <- 0)

  :compare
  (cjump edi < 5 :funcCall :done)

  :funcCall

  ;;generate args and run function
  (eax <- edi)
  (ebx <- eax)
  (ebx *= 2)
  (call :perimeter)

  ;; encode result
  (eax <<= 1)
  (eax += 1)
  
  ;; print
  (eax <- (print eax))
  
  (edi += 1)
  (goto :compare)

  :done)

 (:perimeter
  (length <- eax)
  (height <- ebx)
  (length *= 2)
  (height *= 2)
  (perimeter <- length)
  (perimeter += height)
  (eax <- perimeter)
  (return)))
